<%- model_class = UpFile -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:name) %>:</strong></dt>
  <dd><%= @up_file.name %></dd>
  <dt><strong><%= model_class.human_attribute_name(:fields_enclose) %>:</strong></dt>
  <dd><%= @up_file.fields_enclose %></dd>
  <dt><strong><%= model_class.human_attribute_name(:fields_terminate) %>:</strong></dt>
  <dd><%= @up_file.fields_terminate %></dd>
  <dt><strong><%= model_class.human_attribute_name(:lines_terminate) %>:</strong></dt>
  <dd><%= @up_file.lines_terminate %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              up_files_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_up_file_path(@up_file), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              up_file_path(@up_file),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
